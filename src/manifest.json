{
  "manifest_version": 2,
  "name": "Tripletex Filler",
  "version": "1.0",

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

   "browser_action": {
    "default_icon": "/assets/img/icon.png",
    "default_title": "Tripletex Filler"
  },

  "permissions": [
    "activeTab"
  ]
}