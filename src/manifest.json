{
  "manifest_version": 2,
  "name": "Tripletex Filler",
  "version": "1.0",

  "background": {
    "scripts": [ "extension/background.js" ],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "/extension/icon.png",
    "default_title": "Tripletex Filler"
  },

  "options_page": "/app/index.html#/options",

  "permissions": [
    "activeTab",
    "storage"
  ]
}